{
    "name": "alt3/cakephp-validation-exposer",
    "description": "Easily expose your CakePHP 3.x application validation rules",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.1"
    },
    "support": {
        "issues": "https://github.com/alt3/cakephp-validation-exposer/issues",
        "source": "https://github.com/alt3/cakephp-validation-exposer"
    },
    "autoload": {
        "psr-4": {
            "Alt3\\ValidationExposer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alt3\\Swagger\\Test\\": "tests"
        }
    }
}
